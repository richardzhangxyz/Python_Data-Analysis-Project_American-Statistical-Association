    "#find population sum across all tracts\n",
    "total_pop = food_kid[\"Pop2010\"].sum()\n",
    "print(total_pop)\n",
    "\n",
    "#find sum of kids who are defined as food insecure\n",
    "kid_insecure = food_kid[\"lakids1\"].sum()\n",
    "print(kid_insecure)\n",
    "\n",
    "#find sum of kids in all tracts \n",
    "total_kids = food_kid[\"TractKids\"].sum()\n",
    "print(total_kids)\n",
    "\n",
    "#find proportion and percentage of kids who are food insecure\n",
    "kid_insecure = (kid_insecure)/(total_kids)\n",
    "print(\"Proportion of children who are food insecure is\", kid_insecure)\n",
    "kid_insecure_percent = kid_insecure * 100\n",
    "print(\"Proportion of children who are food insecure is\", kid_insecure_percent, \"% of individuals\")"
